{
  "name": "react-drawio",
  "version": "1.0.4",
  "type": "module",
  "description": "React component for integrating the Diagrams (draw.io) embed iframe",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run build && clean-publish",
    "emit-declarations": "tsc --emitDeclarationOnly",
    "build": "rollup -c && npm run emit-declarations",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcveens/react-drawio.git"
  },
  "author": "Marc Veens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcveens/react-drawio/issues"
  },
  "homepage": "https://github.com/marcveens/react-drawio#readme",
  "tags": [
    "react",
    "diagram",
    "draw.io"
  ],
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-storysource": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/react": "^19.0.5",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "clean-publish": "^5.1.0",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.30.1",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-import-css": "^3.5.8",
    "rollup-plugin-swc3": "^0.12.1",
    "storybook": "^8.4.7",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "exports": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "exports": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "directory": "package"
  },
  "files": [
    "dist"
  ]
}
